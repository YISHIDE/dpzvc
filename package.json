{
  "name": "dpzvc-ui",
  "version": "1.0.0",
  "description": "Vue组件库",
  "main": "dist/dpzvc.js",
  "scripts": {
    "init": "webpack --progress --config webpack.dev.config.js",
    "dev": "webpack-dev-server   --inline --hot --compress --history-api-fallback  --config webpack.dev.config.js",
    "build:style": "gulp --gulpfile build-style.js",
    "build:dev": "webpack --config webpack.dist.dev.config.js",
    "build:prod": "webpack -p --config webpack.dist.prod.config.js",
    "build": "npm run build:style && npm run build:dev && npm run build:prod",
    "dist": "webpack --progress --hide-modules --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yishide/dpzvc.git"
  },
  "keywords": [
    "Vue",
    "Vue 2.x",
    "dpzvc",
    "UI",
    "Components"
  ],
  "dependencies": {
    "china-area-data": "^3.0.0",
    "core-js": "^2.4.1",
    "deepmerge": "^1.3.1",
    "vue": "^2.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "autoprefixer-loader": "^2.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.23.1",
    "eslint": "^3.12.2",
    "eslint-plugin-html": "^1.7.0",
    "exif-js": "^2.2.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.8.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "function-bind": "^1.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.1.0",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^2.28.0",
    "less": "^4.5.1",
    "less-loader": "^2.2.3",
    "lolex": "^1.5.2",
    "postcss-loader": "^2.0.9",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue": "^2.2.1",
    "vue-hot-reload-api": "^1.3.3",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^11.0.0",
    "vue-router": "^2.2.1",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  },
  "author": "yishide",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yishide/dpzvc/issues"
  },
